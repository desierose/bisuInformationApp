<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dashboard">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="369dp"
        android:layout_height="372dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="21dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044"
        app:srcCompat="@drawable/dashbackground" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="154dp"
        android:layout_height="170dp"
        android:layout_marginBottom="88dp"
        app:cardBackgroundColor="#D85C61"
        app:cardCornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cardView4"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="90dp"
            android:layout_height="118dp"
            android:layout_gravity="center"
            android:scaleType="centerInside"
            android:scrollbarSize="4dp"
            app:srcCompat="@drawable/news" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="130dp"
            android:layout_height="56dp"
            android:layout_gravity="bottom|center_horizontal"
            android:fontFamily="sans-serif-medium"
            android:text=" News/
Announcement"
            android:textSize="20dp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="154dp"
        android:layout_height="170dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="84dp"
        app:cardBackgroundColor="#D0E6EF"
        app:cardCornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView2"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="80dp"
            android:layout_height="118dp"
            android:layout_gravity="center"
            android:scaleType="centerInside"
            android:scrollbarSize="4dp"
            app:srcCompat="@drawable/calendar" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="124dp"
            android:layout_height="57dp"
            android:layout_gravity="bottom|center_horizontal"
            android:fontFamily="sans-serif-medium"
            android:text="Calendar integration"
            android:textSize="20dp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:cardCornerRadius="80dp"
        app:circularflow_radiusInDP="0"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="65dp" >

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/menu" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="154dp"
        android:layout_height="170dp"
        app:cardBackgroundColor="#F6AAC4"
        app:cardCornerRadius="20dp"
        app:layout_constraintEnd_toStartOf="@+id/cardView2"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="280dp">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="96dp"
            android:layout_height="118dp"
            android:layout_gravity="center"
            android:scaleType="centerInside"
            android:scrollbarSize="4dp"
            app:srcCompat="@drawable/img" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:fontFamily="sans-serif-medium"
            android:text="Home"
            android:textSize="20dp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="154dp"
        android:layout_height="170dp"
        android:layout_marginEnd="36dp"
        app:cardBackgroundColor="#AB98FA"
        app:cardCornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="280dp">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="80dp"
            android:layout_height="115dp"
            android:layout_gravity="center"
            android:scaleType="centerInside"
            android:scrollbarSize="4dp"
            app:srcCompat="@drawable/eventcreation" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Create Event"
            android:textSize="20dp"
            android:fontFamily="sans-serif-medium"
            android:layout_gravity="bottom|center_horizontal"/>
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Admin's Dashboard"
        android:textSize="30dp"
        android:textColor="#5C4C4C"
        android:fontFamily="sans-serif-medium"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="217dp" />

</androidx.constraintlayout.widget.ConstraintLayout>